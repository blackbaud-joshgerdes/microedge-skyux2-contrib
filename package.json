{
  "name": "microedge-skyux2-contrib",
  "version": "2.2.21",
  "description": "Holding place for Microedge SkyUX 2 components that are yet contributed to the main SkyUX 2 repo.",
  "author": "Brandon Stirnaman <brandon.stirnaman@microedge.com>",
  "homepage": "https://github.com/blackbaud/microedge-skyux2-contrib#readme",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run release",
    "prerelease": "rimraf -- dist",
    "release": "cp -r ./src ./dist && npm run compile:sass",
    "ci": "npm run lint && npm run test:ci && npm run build",
    "build": "npm run clean:srctemp && node config/utils/stage-ts.js && cd .srctemp &&  ../node_modules/.bin/ngc && cd ../ && npm run compile:sass && cd config/rollup && ../../node_modules/.bin/rollup -c rollup.config.js && ../../node_modules/.bin/rollup -c rollup.min.config.js && npm run clean:srctemp && cd ../.. && cp -r ./src/scss ./dist/scss",
    "clean": "rimraf -- dist",
    "clean:full": "npm cache clean && rimraf -- node_modules doc coverage dist .srctemp webdriver-screenshotslocal webdriver-screenshotslocal-diffs",
    "clean:releases": "rimraf -- releases",
    "clean:srctemp": "rimraf -- .srctemp",
    "compile:sass": "node-sass src/scss/sky-contrib.scss dist/css/sky.css",
    "lint": "tslint 'src/**/*.ts'",
    "test": "npm run test:unit && npm run test:visual",
    "test:ci": "npm run test:unit:ci && npm run test:visual:ci",
    "test:unit": "npm run test:unit:base -- config/karma/local.karma.conf.js",
    "test:unit:ci": "npm run test:unit:base -- config/karma/ci.karma.conf.js",
    "test:unit:base": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "test:visual": "npm run test:visual:base -- config/wdio/local.wdio.conf.js",
    "test:visual:ci": "./scripts/browserstack-local-start.sh && npm run test:visual:base -- config/wdio/ci.wdio.conf.js",
    "test:visual:local-browserstack": "./scripts/browserstack-local-start.sh && npm run test:visual:base -- config/wdio/local-browserstack.wdio.conf.js",
    "test:visual:base": "./node_modules/.bin/wdio",
    "postversion": "git push && git push --tags",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "preversion": "npm test",
    "releases": "npm run clean:releases && ./node_modules/.bin/webpack --progress --config config/webpack/webpack.releases.js",
    "start": "webpack-dev-server --config config/webpack/webpack.dev.js --inline --progress --profile --watch --content-base src/",
    "start:visual": "node ./config/utils/visual-server.js start",
    "version": "npm run build",
    "watch": "npm run test:unit -- --auto-watch --no-single-run"
  },
  "dependencies": {
    "@blackbaud/skyux": "2.0.0-beta.31",
    "animate.css": "^3.5.1",
    "angular2localization": "1.1.1",
    "dragula": "3.7.2",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jquery": "^3.1.0",
    "little-loader": "^0.1.1",
    "madlib-shim-easyxdm": "^2.4.20-7",
    "microedge-rxstate": "^1.0.0",
    "mixpanel-browser": "^2.11.1",
    "moment": "^2.18.1",
    "ng2-dragula": "1.2.2-0",
    "ng2-toastr": "0.3.1",
    "normalize-scss": "^6.0.0"
  },
  "peerDependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/router": "3.4.8",
    "core-js": "2.4.1",
    "rxjs": "^5.2.0",
    "zone.js": "^0.7.6"
  },
