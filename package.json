    "@types/node": "6.0.45",
    "@types/webpack": "2.2.4",
    "ajv-keywords": "1.1.1",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "axe-core": "1.1.1",
    "base64-image-loader": "1.0.0",
    "browserstack-local": "1.2.0",
    "codelyzer": "~3.0.1",
