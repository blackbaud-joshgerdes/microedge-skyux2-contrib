{
  "name": "microedge-skyux2-contrib",
  "version": "4.5.5",
  "description": "Holding place for Microedge SkyUX components that are yet contributed to the main SkyUX repo.",
  "homepage": "https://github.com/blackbaud/microedge-skyux2-contrib#readme",
  "main": "bundles/bundle.umd.js",
  "module": "index.ts",
  "engines": {
    "node": ">=6.11.1"
  },
  "scripts": {
    "skyux": "skyux",
    "lint": "skyux lint",
    "test": "skyux test && skyux e2e",
    "copy:scss": "cpx \"src/app/public/src/modules/**/*.scss\" \"dist/src/modules/\" && cpx \"src/app/public/src/scss/**\" \"dist/src/scss/\"",
    "build": "skyux build-public-library && npm run copy:scss",
    "start": "skyux serve"
  },
  "keywords": [],
  "author": "Blackbaud",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^3.5.2",
    "angular-l10n": "^3.4.1",
    "dragula": "3.7.2",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jquery": "^3.1.0",
    "little-loader": "^0.1.1",
    "madlib-shim-easyxdm": "^2.4.20-7",
    "microedge-rxstate": "2.0.0",
    "mixpanel-browser": "^2.11.1",
    "moment": "^2.18.1",
    "ng2-dragula": "1.5.0",
    "ng2-toastr": "4.1.2",
    "normalize-scss": "^7.0.0"
  },
  "devDependencies": {
    "@blackbaud/skyux": "2.0.0-rc.10",
    "@blackbaud/skyux-builder": "1.0.0-rc.15",
    "cpx": "1.5.0",
    "prismjs": "1.6.0"
  },
  "peerDependencies": {
    "@angular/animations": "4.2.5",
    "@angular/common": "4.2.5",
    "@angular/compiler": "4.2.5",
    "@angular/core": "4.2.5",
    "@angular/forms": "4.2.5",
    "@angular/platform-browser": "4.2.5",
    "@angular/platform-browser-dynamic": "4.2.5",
    "@angular/router": "4.2.5",
    "@blackbaud/skyux": "2.0.0-rc.10",
    "core-js": "2.4.1",
    "rxjs": "5.4.2",
    "zone.js": "0.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbaud/microedge-skyux2-contrib.git"
  },
