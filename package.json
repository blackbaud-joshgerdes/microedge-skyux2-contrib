{
  "name": "microedge-skyux2-contrib",
  "version": "4.1.0",
  "description": "Holding place for Microedge SkyUX 2 components that are yet contributed to the main SkyUX 2 repo.",
  "author": "Brandon Stirnaman <brandon.stirnaman@microedge.com>",
  "homepage": "https://github.com/blackbaud/microedge-skyux2-contrib#readme",
  "license": "MIT",
  "main": "dist/bundles/core.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run release",
    "prerelease": "rimraf -- dist",
    "release": "cp -r ./src ./dist && npm run compile:sass",
    "ci": "npm run lint && npm run test:ci && npm run build",
    "build": "npm run clean:srctemp && node config/utils/stage-ts.js && cd .srctemp && ../node_modules/.bin/ngc && cd ../ && npm run compile:sass && cd config/rollup && ../../node_modules/.bin/rollup -c rollup.config.js && ../../node_modules/.bin/rollup -c rollup.min.config.js && npm run clean:srctemp && cd ../.. && cp -r ./src/scss ./dist/scss",
    "clean": "rimraf -- dist",
    "clean:full": "npm cache clean && rimraf -- node_modules doc coverage dist .srctemp webdriver-screenshotslocal webdriver-screenshotslocal-diffs skyux-spa-visual-tests/node_modules/ skyux-spa-visual-tests/dist/",
    "clean:releases": "rimraf -- releases",
    "clean:srctemp": "rimraf -- .srctemp",
    "compile:sass": "node-sass src/scss/sky.scss dist/css/sky.css",
    "lint": "tslint --type-check --project tsconfig.json 'src/**/*.ts'",
    "test": "npm run test:unit && npm run test:visual",
    "test:ci": "npm run test:unit:ci && npm run test:visual:ci",
    "test:unit": "npm run test:unit:base -- config/karma/local.karma.conf.js",
    "test:unit:ci": "npm run test:unit:base -- config/karma/ci.karma.conf.js",
    "test:unit:base": "node --max-old-space-size=4096 node_modules/karma/bin/karma start",
    "test:visual": "cd skyux-spa-visual-tests && npm run build-visual && npm run test-visual:local",
    "test:visual:ci": "cd skyux-spa-visual-tests && npm run build-visual && npm run test-visual:ci",
    "test:visual:local-browserstack": "cd skyux-spa-visual-tests && npm run build-visual && npm run test-visual:local-browserstack",
    "postversion": "git push && git push --tags",
    "prebuild": "npm run clean",
    "pretest": "npm run lint",
    "preversion": "npm test",
    "releases": "npm run clean:releases && ./node_modules/.bin/webpack --progress --config config/webpack/webpack.releases.js",
    "start": "webpack-dev-server --config config/webpack/webpack.dev.js --inline --progress --profile --watch --content-base src/",
    "start:visual": "node ./config/utils/visual-server.js start",
    "version": "npm run build",
    "watch": "npm run test:unit -- --auto-watch --no-single-run"
  },
  "dependencies": {
    "animate.css": "^3.5.1",
    "angular2localization": "1.1.1",
    "dragula": "3.7.2",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "intl-locales-supported": "^1.0.0",
    "jquery": "^3.1.0",
    "little-loader": "^0.1.1",
    "madlib-shim-easyxdm": "^2.4.20-7",
    "microedge-rxstate": "2.0.0",
    "mixpanel-browser": "^2.11.1",
    "moment": "^2.18.1",
    "ng2-dragula": "1.5.0",
    "ng2-toastr": "4.1.2",
    "normalize-scss": "^7.0.0"
  },
  "peerDependencies": {
    "@angular/animations": "4.2.5",
    "@angular/common": "4.2.5",
    "@angular/compiler": "4.2.5",
    "@angular/core": "4.2.5",
    "@angular/forms": "4.2.5",
    "@angular/platform-browser": "4.2.5",
    "@angular/platform-browser-dynamic": "4.2.5",
    "@angular/router": "4.2.5",
    "@blackbaud/skyux": "2.0.0-rc.7",
    "core-js": "2.4.1",
    "rxjs": "5.4.0",
    "zone.js": "0.8.10"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.2.5",
    "@blackbaud/skyux": "2.0.0-rc.7",
    "@blackbaud/skyux-builder": "1.0.0-rc.9",
    "@types/core-js": "0.9.41",
    "@types/jasmine": "2.5.53",
    "@types/node": "8.0.1",
    "@types/webpack": "2.2.16",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "axe-core": "1.1.1",
    "base64-image-loader": "1.2.0",
    "codelyzer": "3.0.1",
    "css-loader": "0.28.1",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "font-awesome-webpack": "0.0.5-beta.2",
    "fontfaceobserver": "2.0.9",
    "fs-extra": "3.0.1",
    "glob": "7.1.1",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.10.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine": "2.6.0",
    "json-loader": "0.5.4",
    "karma": "1.7.0",
    "karma-browserstack-launcher": "1.2.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-firefox-launcher": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.3",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.3",
    "less": "2.7.2",
    "node-resemble-js": "0.2.0",
    "node-sass": "4.5.3",
    "normalize-scss": "^7.0.0",
    "prismjs": "1.6.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "0.9.5",
    "rimraf": "2.6.1",
    "rollup": "0.41.6",
    "rollup-plugin-commonjs": "8.0.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "1.0.2",
    "sass-loader": "6.0.5",
    "selenium-standalone": "6.4.1",
    "source-map-inline-loader": "blackbaud-bobbyearl/source-map-inline-loader",
    "source-map-loader": "0.2.1",
    "style-loader": "0.17.0",
    "ts-helpers": "1.1.2",
    "tslint": "5.4.3",
    "tslint-loader": "3.5.3",
    "typescript": "2.3.2",
    "url-loader": "0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-dev-server": "2.4.5",
    "webpack-merge": "4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbaud/microedge-skyux2-contrib.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/microedge-skyux2-contrib/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
